# Copy to .streamlit/secrets.toml and fill values for production secrets
# Example:
[auth]
# Set username and hashed password (sha256 hex). To create a hash in Python:
# >>> import hashlib
# >>> hashlib.sha256(b"your-password").hexdigest()
username = "admin"
password_hash = "<sha256-hex-hash>"

# [sentry]
# dsn = "https://your_sentry_dsn"
